<contentHaul xmlns:a="http://www.appian.com/ae/types/2009">
    <versionUuid>_a-0000e906-146c-8000-e9f7-01ef9001ef90_128156</versionUuid>
    <rule>
        <name>VAC_COM_generateRandomID</name>
        <uuid>_a-0000e906-146c-8000-e9f7-01ef9001ef90_128137</uuid>
        <description>Generates a random ID (string) of a specified length with specified characters</description>
        <parentUuid>dbecc6ad-7fdc-4080-af31-97861632850a</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>if(
  or(
    isnull(ri!alphabet),
    isnull(ri!length)
  ),
  null,
  joinarray(
    #"SYSTEM_SYSRULES_forEach"(
      items: floor(rand(ri!length)*len(ri!alphabet))+1,
      expression: charat(ri!alphabet,fv!item)
    )
  )
)</definition>
        <namedTypedValue>
            <name>alphabet</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <namedTypedValue>
            <name>length</name>
            <type>
                <name>int</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
