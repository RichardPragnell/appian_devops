<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<contentHaul xmlns:a="http://www.appian.com/ae/types/2009">
    <versionUuid>_a-0000e906-146c-8000-e9f7-01ef9001ef90_138403</versionUuid>
    <rule>
        <name>VAC_COM_getDuration</name>
        <uuid>_a-0000e906-146c-8000-e9f7-01ef9001ef90_138312</uuid>
        <description>Returns a map of the duration between two given dates</description>
        <parentUuid>dbecc6ad-7fdc-4080-af31-97861632850a</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>if(
  or(isnull(ri!startDate),isnull(ri!endDate)),
  null,
  #"_a-0000e906-146c-8000-e9f7-01ef9001ef90_138384"(duration: todecimal(if(
    ri!endDate &gt; ri!startDate,
    ri!endDate - ri!startDate,
    ri!startDate - ri!endDate
  )))
)</definition>
        <namedTypedValue>
            <name>startDate</name>
            <type>
                <name>dateTime</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>endDate</name>
            <type>
                <name>dateTime</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
    <roleMap public="true">
